  "group": [
    {
      "target": "http://terminology.hl7.org/CodeSystem/v3-RoleCode",
      "element": [
        {
          "code": "AdultICU",
          "display": "Adult ICU bed type",
          "target": [
            {
              "code": "ICU",
              "equivalence": "equivalent",
              "comment": "These can support critically ill or injured patients, including ventilator support This category includes all major subtypes of ICU beds, including neurological, cardiac, trauma, or medical, with the exception that this category does not include burn ICU beds"
            }
          ]
        },
        {
          "code": "PediatricICU",
          "display": "Pediatric ICU beds.",
          "target": [
            {
              "code": "PEDICU",
              "equivalence": "equivalent",
              "comment": "This is similar to adult ICU beds, but for patients 17-years-old and younger."
            }
          ]
        },
        {
          "code": "NeonatalICU",
          "display": "Neonatal ICU beds",
          "target": [
            {
              "code": "PEDNICU",
              "equivalence": "equivalent",
              "comment": "Neonatal ICU beds"
            }
          ]
        },
        {
          "code": "ED",
          "display": "Emergency Department beds used for acute care",
          "target": [
            {
              "code": "ER",
              "equivalence": "equivalent",
              "comment": "Emergency Department beds used for acute care"
            }
          ]
        },
        {
          "code": "MedicalSurgical",
          "display": "Medical-surgical beds",
          "target": [
            {
              "code": "HU",
              "equivalence": "equivalent",
              "comment": "These are also thought of as ward beds. These beds may or may not include cardiac telemetry capability"
            }
          ]
        },
        {
          "code": "RehabLongTermCare",
          "display": "Rehabilitation/Long term care beds",
          "target": [
            {
              "code": "RHU",
              "equivalence": "equivalent",
              "comment": "Beds designated as long term care rehabilitation. These do not include floor beds"
            }
          ]
        },
        {
          "code": "Pediatrics",
          "display": "Pediatrics beds.",
          "target": [
            {
              "code": "PEDU",
              "equivalence": "equivalent",
              "comment": "These are ward medical/surgical beds for patients 17-years-old and younger"
            }
          ]
        },
        {
          "code": "Psychiatric",
          "display": "Ward beds on a closed/locked psychiatric unit or ward beds where a patient will be staffed by an attendant.",
          "target": [
            {
              "code": "PHU",
              "equivalence": "equivalent",
              "comment": "Ward beds on a closed/locked psychiatric unit or ward beds where a patient will be staffed by an attendant."
            }
          ]
        },
        {
          "code": "Burn",
          "display": "Burn beds",
          "target": [
            {
              "equivalence": "unmatched"
            }
          ]
        },
        {
          "code": "NurseryBeds",
          "display": "Neonatal or newborn care beds including all bed types other than Neonatal ICU",
          "target": [
            {
              "equivalence": "unmatched"
            }
          ]
        },
        {
          "code": "OperatingRooms",
          "display": "Operating rooms which are equipped, staffed and could be made available for patient care in a short period of time",
          "target": [
            {
              "equivalence": "unmatched"
            }
          ]
        },
        {
          "code": "NegativeFlowIsolation",
          "display": "Negative airflow isolation beds.",
          "target": [
            {
              "equivalence": "unmatched"
            }
          ]
        },
        {
          "code": "OtherIsolation",
          "display": "Isolation beds.",
          "target": [
            {
              "equivalence": "unmatched"
            }
          ]
        },
        {
          "code": "NONISO",
          "display": "Non-Isolation beds.",
          "target": [
            {
              "equivalence": "unmatched"
            }
          ]
        }
      ]
    }
  ]
